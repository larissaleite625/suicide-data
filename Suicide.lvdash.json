{
  "datasets": [
    {
      "name": "bea3e96b",
      "displayName": "Evolução Global (A Tendência Histórica)",
      "queryLines": [
        "SELECT \r\n",
        "    year AS ano,\r\n",
        "    ROUND(global_avg_rate, 2) AS taxa_media_global\r\n",
        "FROM gold.data_suicide.global_metrics\r\n",
        "WHERE year >= 2000\r\n",
        "ORDER BY year;"
      ]
    },
    {
      "name": "17423295",
      "displayName": "A Hipótese Central (Suicídio vs. Tratamento de Drogas)",
      "queryLines": [
        "SELECT \r\n",
        "    s.country_name AS pais,\r\n",
        "    s.year AS ano,\r\n",
        "    ROUND(s.suicide_rate, 2) AS taxa_suicidio,\r\n",
        "    ROUND(s.treatment_coverage, 2) AS cobertura_tratamento_drogas,\r\n",
        "    CASE\r\n",
        "        WHEN s.hdi >= 0.8 THEN 'Very High'\r\n",
        "        WHEN s.hdi >= 0.7 THEN 'High'\r\n",
        "        WHEN s.hdi >= 0.55 THEN 'Medium'\r\n",
        "        ELSE 'Low'\r\n",
        "    END AS categoria_idh\r\n",
        "FROM gold.data_suicide.multifactor_correlation s\r\n",
        "WHERE s.treatment_coverage IS NOT NULL\r\n",
        "  AND s.year = (\r\n",
        "      SELECT MAX(year)\r\n",
        "      FROM gold.data_suicide.multifactor_correlation\r\n",
        "      WHERE treatment_coverage IS NOT NULL\r\n",
        "  );"
      ]
    },
    {
      "name": "00736fae",
      "displayName": "Disparidade de Gênero (O \"Quem\")",
      "queryLines": [
        "SELECT \r\n",
        "    country_name AS pais,\r\n",
        "    ROUND(male_rate, 2) AS taxa_masculina,\r\n",
        "    ROUND(female_rate, 2) AS taxa_feminina,\r\n",
        "    ROUND(gender_ratio, 2) AS razao_homem_mulher\r\n",
        "FROM gold.data_suicide.gender_disparity_analysis\r\n",
        "WHERE year = (SELECT MAX(year) FROM gold.data_suicide.gender_disparity_analysis)\r\n",
        "ORDER BY razao_homem_mulher DESC\r\n",
        "LIMIT 10;"
      ]
    },
    {
      "name": "77406102",
      "displayName": "Hotspots de Risco (O \"Onde\")",
      "queryLines": [
        "SELECT \r\n",
        "    country_name AS pais,\r\n",
        "    year AS ano,\r\n",
        "    ROUND(suicide_rate, 2) AS taxa_atual,\r\n",
        "    ROUND(rate_change_pct, 2) AS mudanca_1_ano_pct,\r\n",
        "    alert_type AS tipo_alerta\r\n",
        "FROM gold.data_suicide.significant_changes_alerts\r\n",
        "WHERE year = (SELECT MAX(year) FROM gold.data_suicide.significant_changes_alerts)\r\n",
        "  AND alert_type LIKE '%Increase%' -- Focar em quem está piorando\r\n",
        "ORDER BY mudanca_1_ano_pct DESC\r\n",
        "LIMIT 15;"
      ]
    },
    {
      "name": "70c1a59e",
      "displayName": "Untitled dataset",
      "queryLines": [
        "-- 1. Taxa do Brasil\r\n",
        "(\r\n",
        "SELECT \r\n",
        "    year AS ano,\r\n",
        "    'Brasil' AS entidade,\r\n",
        "    ROUND(suicide_rate, 2) AS taxa\r\n",
        "FROM gold.data_suicide.suicide_hdi_analysis\r\n",
        "WHERE country_code = 'BRA'\r\n",
        "  AND year >= 2000\r\n",
        ")\r\n",
        "\r\n",
        "UNION ALL\r\n",
        "\r\n",
        "-- 2. Taxa dos EUA\r\n",
        "(\r\n",
        "SELECT \r\n",
        "    year AS ano,\r\n",
        "    'EUA' AS entidade,\r\n",
        "    ROUND(suicide_rate, 2) AS taxa\r\n",
        "FROM gold.data_suicide.suicide_hdi_analysis\r\n",
        "WHERE country_code = 'USA'\r\n",
        "  AND year >= 2000\r\n",
        ")\r\n",
        "\r\n",
        "UNION ALL\r\n",
        "\r\n",
        "-- 3. Média Global\r\n",
        "(\r\n",
        "SELECT \r\n",
        "    year AS ano,\r\n",
        "    'Média Global' AS entidade,\r\n",
        "    ROUND(global_avg_rate, 2) AS taxa\r\n",
        "FROM gold.data_suicide.global_metrics\r\n",
        "WHERE year >= 2000\r\n",
        ")\r\n",
        "\r\n",
        "UNION ALL\r\n",
        "\r\n",
        "-- 4. Média Américas\r\n",
        "(\r\n",
        "SELECT DISTINCT -- Usar DISTINCT para pegar a média regional apenas uma vez por ano\r\n",
        "    year AS ano,\r\n",
        "    'Média Américas' AS entidade,\r\n",
        "    ROUND(regional_avg_rate, 2) AS taxa\r\n",
        "FROM gold.data_suicide.country_rankings\r\n",
        "WHERE region = 'Americas' -- Filtro pela região\r\n",
        "  AND year >= 2000\r\n",
        ")\r\n",
        "\r\n",
        "UNION ALL\r\n",
        "\r\n",
        "-- 5. Média BRICS\r\n",
        "(\r\n",
        "SELECT \r\n",
        "    year AS ano,\r\n",
        "    'Média BRICS' AS entidade,\r\n",
        "    ROUND(AVG(suicide_rate), 2) AS taxa\r\n",
        "FROM gold.data_suicide.suicide_hdi_analysis\r\n",
        "WHERE country_code IN ('BRA', 'RUS', 'IND', 'CHN', 'ZAF') -- Países BRICS\r\n",
        "  AND year >= 2000\r\n",
        "GROUP BY year\r\n",
        ")\r\n",
        "\r\n",
        "ORDER BY ano, entidade;"
      ]
    }
  ],
